		{
			"name": "Ben Schwartz",
			"email": "benschw@gmail.com"
		}
	],
	"require": {
		"php"                      : ">=5.6",
		"php-amqplib/php-amqplib"  : "2.6.*",
		"fliglio/http"             : "2.3.*",
		"fliglio/web"              : "2.3.*"
	},
	"require-dev": {
		"phpunit/phpunit"      : "5.*",
		"fliglio/fli"          : "2.3.*",
		"fliglio/flfc"         : "2.3.*",
		"fliglio/routing"      : "2.3.*",
		"fliglio/http"         : "2.3.*",
		"fliglio/web"          : "2.3.*",
		"fliglio/chinchilla"   : "2.3.*",
